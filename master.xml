<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- Yocto -->
  <remote name="yocto"
          fetch="git://git.yoctoproject.org"/>
  <project name="poky"
           remote="yocto"
           revision="master"
           upstream="master"
           path="sources/poky"/>
  <project name="meta-java"
           remote="yocto"
           revision="master"
           upstream="master"
           path="sources/meta-java"/>
  <project name="meta-qcom"
           remote="yocto"
           revision="master"
           upstream="master"
           path="sources/meta-qcom"/>

  <!-- OpenEmbedded -->
  <remote name="oe"
          fetch="git://git.openembedded.org"/>
  <remote name="oe-mirror"
          fetch="https://github.com/openembedded"/>
  <project name="meta-openembedded"
           remote="oe"
           revision="master"
           upstream="master"
           path="sources/meta-openembedded"/>

  <!-- Qt -->
  <remote name="qt"
          fetch="https://code.qt.io/yocto/"/>
  <project name="meta-qt6"
           remote="qt"
           revision="6.6"
           upstream="master"
           path="sources/meta-qt6"/>

  <!-- KDE -->
  <remote name="kde"
          fetch="https://invent.kde.org/packaging/"/>
  <project name="yocto-meta-kf6"
           remote="kde"
           revision="master"
           upstream="master"
           path="sources/meta-kf6"/>
  <project name="yocto-meta-kde"
           remote="kde"
           revision="work/qt6"
           upstream="work/qt6"
           path="sources/meta-kde"/>
  <project name="yocto-meta-kde-demo"
           remote="kde"
           revision="master"
           upstream="master"
           path="sources/meta-kde-demo">
    <linkfile dest="setup-environment" src="setup-environment"/>
  </project>

  <!-- Raspberry Pi -->
  <project name="meta-raspberrypi"
           remote="yocto"
           revision="master"
           upstream="master"
           path="sources/meta-raspberrypi"/>

  <!-- RISC-V-->
  <remote name="riscv"
          fetch="https://github.com/riscv/"/>
  <project name="meta-riscv"
           remote="riscv"
           revision="master"
           upstream="master"
           path="sources/meta-riscv"/>
</manifest>
